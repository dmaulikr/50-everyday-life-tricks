cales[scales.length - 1] || 1;\n  }\n\n}\n\n module.exports = AssetSourceResolver;\n"}}},"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Image/ImageSource.js":{"metadata":{"mtime":1479891645000},"data":{"name":"ImageSource"}},"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Image/GlobalImageStub.js":{"metadata":{"mtime":1479891645000},"data":{"name":"GlobalImageStub"}},"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Image/ImageResizeMode.js":{"metadata":{"mtime":1479891645000},"data":{"name":"ImageResizeMode","j9aQyRRX1lcwTe9ow3z1bQ==\u0000moduleData":{"dependencyOffsets":[48],"dependencies":["fbjs/lib/keyMirror"],"code":"\n\n\n\n\n\n\n\n\n\n\n'use strict';\n\nvar keyMirror=require('fbjs/lib/keyMirror');\n\n\n\n\n\nvar ImageResizeMode=keyMirror({\n\n\n\n\ncontain:null,\n\n\n\n\ncover:null,\n\n\n\n\n\nstretch:null,\n\n\n\n\n\ncenter:null,\n\n\n\n\n\nrepeat:null});\n\n\nmodule.exports=ImageResizeMode;","map":null,"transformFileStartLogEntry":{"action_name":"Transforming file","action_phase":"start","file_name":"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Image/ImageResizeMode.js","log_entry_label":"Transforming file","start_timestamp":[8695,493417823]},"transformFileEndLogEntry":{"action_name":"Transforming file","action_phase":"end","file_name":"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Image/ImageResizeMode.js","duration_ms":10,"log_entry_label":"Transforming file"},"id":"ImageResizeMode","source":"/**\n * Copyright (c) 2015-present, Facebook, Inc.\n * All rights reserved.\n *\n * This source code is licensed under the BSD-style license found in the\n * LICENSE file in the root directory of this source tree. An addi