 with concat method).\n  if (Array.isArray(current)) {\n    if (Array.isArray(next)) {\n      current.push.apply(current, next);\n      return current;\n    }\n    current.push(next);\n    return current;\n  }\n\n  if (Array.isArray(next)) {\n    // A bit too dangerous to mutate `next`.\n    return [current].concat(next);\n  }\n\n  return [current, next];\n}\n\nmodule.exports = accumulateInto;\n"}}},"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Renderer/src/renderers/shared/utils/isTextInputElement.js":{"metadata":{"mtime":1479891645000},"data":{"name":"isTextInputElement"}},"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Renderer/src/renderers/shared/utils/accumulate.js":{"metadata":{"mtime":1479891645000},"data":{"name":"accumulate","j9aQyRRX1lcwTe9ow3z1bQ==\u0000moduleData":{"dependencyOffsets":[49],"dependencies":["fbjs/lib/invariant"],"code":"\n\n\n\n\n\n\n\n\n\n\n\n'use strict';\n\nvar invariant=require('fbjs/lib/invariant');\n\n\n\n\n\n\n\n\nfunction accumulate(current,next){\ninvariant(\nnext!=null,\n'accumulate(...): Accumulated items must be not be null or undefined.');\n\n\nif(current==null){\nreturn next;\n}\n\n\n\nif(Array.isArray(current)){\nreturn current.concat(next);\n}\n\nif(Array.isArray(next)){\nreturn[current].concat(next);\n}\n\nreturn[current,next];\n}\n\nmodule.exports=accumulate;","map":null,"transformFileStartLogEntry":{"action_name":"Transforming file","action_phase":"start","file_name":"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/node_modules/react-native/Libraries/Renderer/src/renderers/shared/utils/accumulate.js","log_entry_label":"Transforming file","start_timestamp":[8703,169437232]},"transformFileEndLogEntry":{"action_name":"Transforming file","action_phase":"end","file_name":"/home/manparvesh/Desktop/github_repos/50-everyday-life-tricks/EverydayLifeTricks/n